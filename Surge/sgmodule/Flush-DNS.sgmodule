#!name=Flush DNS
#!desc=快速清除Surge的DNS缓存
#!system=ios

[Panel]
FlushDNS = script-name=FlushDNS,update-interval=600

[Script]
FlushDNS = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/jijunrong/surge/main/Surge/js/Flush-DNS.js,argument=title=DNS FLush&icon=cube&color=#afe59a&server=false